{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/lobby",
      "serve": "/lobby.html"
    },
    {
      "route": "/index*",
      "serve": "/index.html"
    },
    {
      "route": "*.ts",
      "headers": {
        "Content-Type": "text/javascript"
      }
    }
  ]
}